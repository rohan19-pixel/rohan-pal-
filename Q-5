class Vehicle:
    def calculate_fare(self, km):
        return 0

class Auto(Vehicle):
    def calculate_fare(self, km):
        return km * 12

class Bike(Vehicle):
    def calculate_fare(self, km):
        return km * 8

class Sedan(Vehicle):
    def calculate_fare(self, km):
        return km * 18

for v in [Auto(), Bike(), Sedan()]:
    print(v.calculate_fare(10))
